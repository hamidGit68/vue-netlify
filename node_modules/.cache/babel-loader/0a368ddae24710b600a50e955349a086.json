AA,oBAAA;AACA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KAJA;AAIA;AAEA,IAAA,YAAA,EAAA,wBAAA;AACA;AACA,MAAA,CAAA,CAAA,gBAAA,CAAA,CAAA,WAAA,CAAA,WAAA,EAFA,CAGA;;AACA,MAAA,CAAA,CAAA,8BAAA,KAAA,KAAA,GAAA,GAAA,CAAA,CAAA,QAAA,CAAA,WAAA,EAJA,CAMA;;AACA,MAAA,CAAA,CAAA,oBAAA,CAAA,CAAA,WAAA,CAAA,QAAA,EAPA,CAQA;;AACA,MAAA,CAAA,CAAA,kCAAA,KAAA,KAAA,GAAA,GAAA,CAAA,CAAA,QAAA,CAAA,QAAA;AAEA,WAAA,KAAA;;AACA,UAAA,KAAA,KAAA,GAAA,KAAA,OAAA,EAAA;AACA,aAAA,KAAA,GAAA,CAAA;AACA;AACA,KArBA;AAuBA,IAAA,YAAA,EAAA,wBAAA;AACA,WAAA,OAAA,GAAA,WAAA,CAAA,KAAA,YAAA,EAAA,IAAA,CAAA;AACA;AAzBA,GA/BA;AA2DA,EAAA,aA3DA,2BA2DA;AACA,IAAA,aAAA,CAAA,KAAA,OAAA,CAAA;AACA,GA7DA;AA+DA,EAAA,OA/DA,qBA+DA;AACA,SAAA,YAAA;AACA;AAjEA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container-fluid\">\r\n    <div class=\"row\">\r\n      <div class=\"col-md-12\">\r\n        <div id=\"parent\" class=\"jumbotron\">\r\n          <ul id=\"ul-service\">\r\n            <li v-for=\"item in servicData\" :key=\"item.id\">\r\n              {{ item.message }}\r\n            </li>\r\n          </ul>\r\n          <!-- showing icons related to texts -->\r\n          <div id=\"icons\">\r\n            <div class=\"iconParent\" v-for=\"item in servicData\" :key=\"item.id\">\r\n              <div class=\"iconCircle\">\r\n                <i :class=\"item.icon\"></i>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"row\">\r\n          <div class=\"col-md-12\">\r\n            <div class=\"buttonParent\">\r\n              <button @click=\"routFunc\" class=\"btn btn-primary\">\r\n                جستجو در فروشگاه سایت\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"servicCompo\",\r\n  data() {\r\n    return {\r\n      polling: null,\r\n      liNum: 1,\r\n      totalLi: 4,\r\n      servicData: [\r\n        {\r\n          message: \"مقالات مرتبط با موضوع کتاب\",\r\n          id: \"1\",\r\n          icon: \"fas fa-feather-alt\"\r\n        },\r\n        {\r\n          message: \"امکان جستجوی پیشرفته\",\r\n          id: \"2\",\r\n          icon: \"fab fa-searchengin\"\r\n        },\r\n        {\r\n          message: \"تخفیف های خاص در ایام سال\",\r\n          id: \"3\",\r\n          icon: \"fas fa-money-bill\"\r\n        },\r\n        {\r\n          message: \"کتب علمی تخصصی دانشجویان\",\r\n          id: \"4\",\r\n          icon: \"fas fa-graduation-cap\"\r\n        }\r\n      ]\r\n    };\r\n  },\r\n  methods: {\r\n    routFunc: function() {\r\n      /* redirect to bookStore page */\r\n      this.$router.push({ name: \"bookStore\" });\r\n    }, // end of \"routFunc\" method\r\n\r\n    activeChange: function() {\r\n      // eslint-disable-next-line no-undef\r\n      $(\"#ul-service li\").removeClass(\"active-li\");\r\n      // eslint-disable-next-line no-undef\r\n      $(\"#ul-service li:nth-child(\" + this.liNum + \")\").addClass(\"active-li\");\r\n\r\n      // eslint-disable-next-line no-undef\r\n      $(\"#icons .iconParent\").removeClass(\"iconLi\");\r\n      // eslint-disable-next-line no-undef\r\n      $(\"#icons .iconParent:nth-child(\" + this.liNum + \")\").addClass(\"iconLi\");\r\n\r\n      this.liNum++;\r\n      if (this.liNum > this.totalLi) {\r\n        this.liNum = 1;\r\n      }\r\n    },\r\n\r\n    interValFunv: function() {\r\n      this.polling = setInterval(this.activeChange, 4000);\r\n    }\r\n  },\r\n\r\n  beforeDestroy() {\r\n    clearInterval(this.polling);\r\n  },\r\n\r\n  created() {\r\n    this.interValFunv();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped src=\"./compoStyles/services.css\"></style>\r\n"],"sourceRoot":"src/components"}]}